{
  "name": "back_end_frontend",
  "version": "0.2.0",
  "description": "Internet Computer Sveltekit starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister",
    "Sveltekit"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "dfx generate",
    "start": "webpack serve --mode development --env development",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate && node ./update.types.mjs",
    "startdfx": "dfx start --clean --background",
    "deploy": "dfx deploy backend --argument '(variant { regtest })' && dfx deploy frontend",
    "deployTransactionCanister": "dfx deploy backendTransaction --argument '(\"n1fE2ysMk2mp3zQdPewNp3hex62wKB7UJr\")'",
    "deployBackend": "dfx deploy backend --argument '(variant { regtest })' && dfx deploy backendTransaction --argument '(\"n1fE2ysMk2mp3zQdPewNp3hex62wKB7UJr\")'",
    "startBTC": "./bin/bitcoind -conf=$(pwd)/bitcoin.conf -datadir=$(pwd)/data --port=18444"
  },
  "dependencies": {
    "@dfinity/agent": "^0.19.3",
    "@dfinity/candid": "^0.19.3",
    "@dfinity/principal": "^0.19.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@rollup/plugin-inject": "^5.0.5",
    "bits-ui": "^0.15.1",
    "clsx": "^2.1.0",
    "mode-watcher": "^0.1.2",
    "radix-icons-svelte": "^1.2.1",
    "svelte-sonner": "^0.3.12",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.1.20",
    "vaul-svelte": "^0.1.0"
  },
  "devDependencies": {
    "@svelte-put/qr": "^1.0.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.4",
    "assert": "2.0.0",
    "autoprefixer": "^10.4.16",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.3",
    "events": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "postcss": "^8.4.31",
    "process": "0.11.10",
    "sass": "^1.69.5",
    "stream-browserify": "3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.5",
    "terser-webpack-plugin": "^5.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "util": "0.12.4",
    "vite": "^4.4.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || >=17",
    "npm": "^7.17 || >=8"
  },
  "type": "module",
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
